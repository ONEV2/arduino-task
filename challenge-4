void setup()
//set pins 3,5,6,9 as output
{
  pinMode(3, OUTPUT);
  pinMode(5, OUTPUT);
  pinMode(6, OUTPUT);
  pinMode(9, OUTPUT);
}

void loop()
{
  analogWrite(3, analogRead(A3));
  analogWrite(5, analogRead(A2));
  analogWrite(6, analogRead(A1));
  analogWrite(9, analogRead(A0));
  delay(10); // Delay a little bit to improve simulation performance
}
